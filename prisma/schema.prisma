// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql" // ( sesi-4)
  url      = env("DATABASE_URL")
}

// make book model 
model book {
  id         Int    @id @default(autoincrement())
  judul_buku String
  deskripsi  String
  category   String
}
